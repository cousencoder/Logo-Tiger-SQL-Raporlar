CREATE TRIGGER ONUR_INS_UPDATE ON LG_024_01_INVOICE
FOR INSERT,UPDATE
AS
BEGIN


DECLARE @LOGICALREF INT
DECLARE @FICHENO NVARCHAR(100)

SELECT @LOGICALREF=LOGICALREF,@FICHENO=FICHENO FROM inserted
 IF @FICHENO IS NOT NULL
 BEGIN
 UPDATE LG_024_01_INVOICE SET DOCODE = @FICHENO WHERE LOGICALREF = @LOGICALREF
 END

END
